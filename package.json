    "body-parser": "^2.2.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "graphql": "^16.10.0",
    "next": "15.2.1",
    "node-fetch": "2",
    "openai": "^4.91.1",
    "pocketbase": "^0.25.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.88.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/axios": "^0.14.4",
    "@types/cheerio": "^0.22.35",
    "@types/express": "4.17.13",
    "@types/node": "^22.14.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chalk": "^5.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
